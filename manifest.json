{
  "manifest_version": 3,
  "name": "Pack your ella",
  "version": "1.0",
  "icons": {
    "16": "images/umbrella16.png",
    "32": "images/umbrella32.png",
    "64": "images/umbrella64.png",
    "128": "images/umbrella128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "geolocation",
    "notifications",
    "alarms"
  ]
}